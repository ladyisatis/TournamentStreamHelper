{
  "name": "tournamentstreamhelper",
  "private": true,
  "version": "6.0.0",
  "type": "module",
  "scripts": {
    "setup": "python3 -m venv venv && . venv/bin/activate && pip install poetry && poetry install && deactivate",
    "setup:win": "python -m venv venv && .\\venv\\Scripts\\activate && pip install poetry && poetry install && .\\venv\\Scripts\\deactivate",
    "dev": "concurrently -n vite,server \"npm run vite\" \"npm run server\"",
    "dev:win": "concurrently -n vite,server \"npm run vite\" \"npm run server:win\"",
    "server": ". venv/bin/activate && venv/bin/python3 main.py && deactivate",
    "server:win": ".\\venv\\Scripts\\activate && python main.py && .\\venv\\Scripts\\deactivate",
    "build": "vite build",
    "vite": "vite",
    "vite:lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "vite:preview": "vite preview"
  },
  "dependencies": {
    "@fontsource/material-icons": "^5.0.18",
    "@fontsource/roboto": "^5.0.13",
    "@mui/material": "^5.15.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.6.8",
    "react-router-dom": "^6.23.1",
    "react-use-websocket": "^4.8.1",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "prop-types": "^15.8.1",
    "vite": "^5.2.0"
  }
}
